    "scripts": {
        "setup": "yarn && yarn --cwd ./client/",
        "upgrade": "yarn upgrade && yarn --cwd ./client/ upgrade",
        "start": "node server/app.js",
        "server": "nodemon server/app.js",
        "client": "yarn --cwd ./client/ start",
        "dev": "concurrently \"yarn run server\" \"yarn run client\""
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "multer": "^1.4.2",
        "mysql2": "^2.2.5",
        "nanoid": "^3.1.23",
        "sharp": "^0.28.3"
    },
    "devDependencies": {
        "concurrently": "^6.2.0",
        "dotenv": "^10.0.0",
        "nodemon": "^2.0.9"
    }
}
